  .mblexploreglry.mblexploreglry2 img { max-width: 138px; }
  .route_tabbing_details .supertabs ul { gap: 26px; }
  .busseatmodify .buslighttext { font-size: 14px; }
  .busseatmodify .windowseat { font-size: 13px; }
  .busfacilitiestab .nav-tabs .nav-item.nav-item-button .nav-link { padding: 4px 24px; }
  .collab-slide { width: 290px; height: 270px; }
  .collabscont { height: 175px; background-size: 152px; padding: 15px 15px 0px; }
  .collabscont h3 { line-height: 22px; font-size: 17px; }
  .collabscont p { font-size: 13px; line-height: 17px; }
  .collabscont span { font-size: 13px; }
}

@media only screen and (max-width: 340px) {
  .sliderwrapper { background-position: center -10px; height: 387px; }
}
